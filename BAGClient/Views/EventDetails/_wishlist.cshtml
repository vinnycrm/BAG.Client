@model BAG.CustomObject.WishList[]
@{
    if (Model != null)
    {
        if(Model.Count()>0)
        { 
        <script>
            window.onload = function () {
                EventSummary('@Model[0].Id')
            };
        </script>
        }
    }
}
<div class="list-group">
    @foreach (var item in Model)
    {
        <div class="list-group-item">
            <a style="text-decoration:none;" href="javascript:EventSummary('@item.Id')">
                <i class="fa fa-tasks fa-fw"></i> @item.WList_Name
            </a>
            <a href="javascript:P_EditWishlist('@item.Id')" class="pull-right">
                <i data-placement="top" data-toggle="tooltip" title="Edit">
                    <span class="btn btn-default btn-xs" data-title="Edit" data-toggle="modal" data-target="#edit">
                        <span class="glyphicon glyphicon-pencil"></span>
                    </span>
                </i>
            </a>
            <a href="javascript:P_DeleteWishlist('@item.Id')" class="pull-right">
                <i data-placement=" top" data-toggle="tooltip" title="Delete">
                    <span class="btn btn-danger btn-xs">
                        <span class="glyphicon glyphicon-trash"></span>
                    </span>
                </i>
            </a>
        </div>
    }
</div>


